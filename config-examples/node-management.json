{
  "orchestrator": { // rename to Orchestration
    "DynamicManagementConfig": {
      "Url": "http://1.1.1.1/reader/node-management",
      "ReadInterval": "1m",
      "ResetTimeout": "30m"
    },
    "storage-driver": "nfs", // rename to StorageDriver
    "storage-options": { // rename to StorageOptions
      "maxRetries": "10"
    }
  },
  "chains": [ // rename to VirtualChains
    {
      "Id":         42,
      "HttpPort":   8080,
      "GossipPort": 4400,
      "Disabled": false,
      "DockerConfig": {
        "ContainerNamePrefix": "orbs-network",
        "Image":  "orbsnetwork/node",
        "Tag":    "v1.1.0",
        "Pull":   true,
        "Resources": {
          "Limits": {
            "Memory": 1024,
            "CPUs": 1
          },
          "Reservations": {
            "Memory": 512,
            "CPUs": 0.5
          }
        }
      },
      "Config": {
        "ManagementConfigUrl": "http://1.1.1.1/vchains/42/management",
        "SignerUrl": "http://1.1.1.1/signer"           
      }
    }
  ],
  "services": { // rename to NodeServices
    "signer": {
      "Port": 7777, // rename to InternalPort
      "ExternalPort": 7778,
      "DockerConfig": {
        "ContainerNamePrefix": "signer",
        "Image":  "orbsnetwork/orbs-network-signer",
        "Tag":    "v1.1.0",
        "Pull":   true
      },
      "Config": {
        "api": "v1"
      }
    },
    "management-reader": {
      "Port": 7776, // rename to InternalPort
      "DockerConfig": {
        "ContainerNamePrefix": "reader",
        "Image":  "orbsnetwork/network-state-reader",
        "Tag":    "v1.1.0",
        "Pull":   true,
      },
      "Config": {
        "EthereumNetwork": "mainnet",
        "EthereumGenesisContract": "0x2384723487623784638272348",
        "EthereumEndpoint": "2.2.2.2:30245",
        "DockerRegistry": "registry.hub.docker.com",
        "DockerNamespace": "orbsnetwork"
      }
    }
  }
}
